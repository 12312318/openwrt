config wifi-device 'mt7615e5'
	option type 'mt7615e5'
	option txantenna '2'
	option rxantenna '2'
	option hwmode '11ac'
	option channel 'auto'
	option htmode 'VHT80'
	option country 'CN'
	option txpower '20'
	option band '5G'
	option region '0'
	option aregion '10'
	option noscan '1'

config wifi-iface
	option device 'mt7615e5'
	option ifname 'ra0'
	option network 'lan'
	option mode 'ap'
	option ssid 'mt7615e5'
	option encryption 'psk2'
	option key '12345678'

config wifi-device 'mt7615e2'
	option type 'mt7615e2'
	option txantenna '2'
	option rxantenna '2'
	option hwmode '11ng'
	option channel 'auto'
	option htmode 'HT40'
	option country 'CN'
	option txpower '20'
	option band '2G'
	option region '0'
	option aregion '10'
	option noscan '1'

config wifi-iface
	option device 'mt7615e2'
	option ifname 'rax0'
	option network 'lan'
	option mode 'ap'
	option ssid 'mt7615e2'
	option encryption 'psk2'
	option key '12345678'
